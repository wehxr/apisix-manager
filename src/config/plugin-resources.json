{
  "resourceTypes": {
    "route": "路由",
    "global_rule": "全局规则",
    "consumer": "消费者",
    "upstream": "上游服务",
    "consumer_group": "消费者组"
  },
  "plugins": {
    "request-id": {
      "name": "生成请求ID",
      "resources": ["route", "global_rule"]
    },
    "consumer-restriction": {
      "name": "用户认证",
      "resources": ["route"]
    },
    "basic-auth": {
      "name": "基本认证",
      "resources": ["consumer"]
    },
    "ip-restriction": {
      "name": "IP 限制",
      "resources": ["route", "consumer"]
    },
    "cors": {
      "name": "跨域策略",
      "resources": ["route", "global_rule"]
    },
    "real-ip": {
      "name": "真实IP透传",
      "resources": ["route", "global_rule"]
    },
    "proxy-rewrite": {
      "name": "代理重写",
      "resources": ["route"]
    },
    "redirect": {
      "name": "重定向",
      "resources": ["route"]
    },
    "gzip": {
      "name": "Gzip压缩",
      "resources": ["route", "global_rule"]
    },
    "proxy-cache": {
      "name": "代理缓存",
      "resources": ["route"]
    },
    "client-control": {
      "name": "限制请求体大小",
      "resources": ["route"]
    },
    "uri-blocker": {
      "name": "URI拦截",
      "resources": ["route"]
    },
    "api-breaker": {
      "name": "API熔断",
      "resources": ["route"]
    },
    "limit-req": {
      "name": "请求速率限流",
      "resources": ["route"]
    },
    "limit-count": {
      "name": "请求数限流",
      "resources": ["route", "consumer"]
    },
    "limit-conn": {
      "name": "并发连接数限制",
      "resources": ["route"]
    }
  }
}
